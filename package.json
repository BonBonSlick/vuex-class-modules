{
  "name": "vuex-class-modules",
  "version": "1.0.1",
  "description": "Typescript class decorators for class-style vuex modules.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "lint": "tslint -p tsconfig.json --fix",
